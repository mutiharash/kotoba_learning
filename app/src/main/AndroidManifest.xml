        <activity android:name="com.unikom.kotobalearning.highscore.HighScoreKataKerja"
            android:parentActivityName=".kuiskosakata.KuisKataKerja">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name="com.unikom.kotobalearning.highscore.HighScoreKataSifat"
