                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name="com.unikom.kotobalearning.kuiskosakata.KuisKataBenda"
            android:parentActivityName=".kuiskosakata.KuisKosakata">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name="com.unikom.kotobalearning.kuiskosakata.KuisKataKerja"
