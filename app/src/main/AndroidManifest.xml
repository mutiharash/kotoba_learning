            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name="com.unikom.kotobalearning.daftarkosakata.KataKerja"
            android:parentActivityName=".daftarkosakata.DaftarKosakata">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name="com.unikom.kotobalearning.daftarkosakata.KataSifat"
            android:parentActivityName=".daftarkosakata.DaftarKosakata">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name="com.unikom.kotobalearning.kuiskosakata.KuisKosakata"
            android:parentActivityName=".MainActivity">
